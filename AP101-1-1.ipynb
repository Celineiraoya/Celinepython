    "def exchange_money(budget, exchange_rate):\n",
    "    exchanged_rate = budget * exchange_rate\n",
    "    return exchanged_rate\n",
    "\n",
    "budget_amount = 100\n",
    "exchange_rate = 0.85\n",
    "result = exchange_money(budget_amount , exchange_rate)\n",
    "print(f'The value of the exchanged currency is: {result}')\n"
